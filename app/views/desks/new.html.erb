<div class="container pt-5">
  <div class="row justify-content-center">
    <div id="column" class="col-12">
      <h1>Create a new Desk</h1>
      <%= simple_form_for @desk do |f| %>
      <%= f.input :title %>
      <%= f.input :seats %>
      <%= f.input :price %>
      <%= f.input :photo, as: :file %>
      <%= f.input :location %>
      <%= f.input :start_date, as: :string, required: false, input_html: {class: "datepicker"} %>
      <%= f.input :end_date, as: :string, required: false, input_html: {class: "datepicker"} %>
      <%= f.button :submit, class: 'btn btn-success' %>
      <% end %>

    </div>
  </div>
</div>
<br>
