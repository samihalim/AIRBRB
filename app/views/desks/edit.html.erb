<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-8">
      <h1>Update Desk</h1>

      <%= simple_form_for @desk do |f| %>
        <%= f.input :title %>
        <%= f.input :seats %>
        <%= f.input :price %>
        <%= f.input :location %>
        <%= f.input :start_date %>
        <%= f.input :end_date %>
        <%= f.button :submit, class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
